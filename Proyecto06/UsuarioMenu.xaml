<Window x:Class="Proyecto05.UsuarioMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto05"
        mc:Ignorable="d"
        Title="UsuarioMenu" Height="400" Width="600">
    <Grid>
        <StackPanel Orientation="Horizontal" Margin="0,41,0,297" HorizontalAlignment="Center">
            <TextBlock Text="Bienvenido Usuario" Height="auto" VerticalAlignment="Center" FontSize="30"/>
        </StackPanel>

        <StackPanel Margin="80,119,80,36" >
            <StackPanel Orientation="Horizontal" Height="108" Width="123">
                <StackPanel.Background>
                    <ImageBrush ImageSource="/Usuario.png" Stretch="UniformToFill" TileMode="None"/>
                </StackPanel.Background>
            </StackPanel>
        </StackPanel>

    </Grid>
</Window>
